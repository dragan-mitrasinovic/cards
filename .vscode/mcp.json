{
  "servers": {
    // For more information, visit: https://angular.dev/ai/mcp
    "angular-cli": {
      "command": "npx",
      "args": ["-y", "@angular/cli", "mcp"]
    },
    // Go documentation via `go doc` and `go list`
    // https://github.com/wricardo/go-mcp
    "go-mcp": {
      "command": "go-mcp",
      "env": {
        "WORKDIR": "${workspaceFolder}/server"
      }
    }
  }
}
